    "semver": "^7.7.2",
    "type-fest": "^4.41.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.7.3",
    "@types/node": "^24.3.0",
    "@types/semver": "^7.7.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.33.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
